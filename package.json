{
	"name": "archopen",
	"version": "0.6.0",
	"description": "Open your roms in retroarch via the windows explorer easier.",
	"main": "src/ArchOpen.js",
	"bin": "src/ArchOpen.js",
	"scripts": {
		"build": "node build.js",
		"setup": "npm i && npm i pkg -g",
		"test": "npm run build && cd dist && ArchOpen.exe \"..\\examples\\2048.nes\"",
		"generate-config": "cd dist && ArchOpen.exe --generate-config",
		"help": "cd dist && ArchOpen.exe -h"
	},
	"keywords": [
		"roms",
		"retroarch",
		"gaming"
	],
	"author": "ZombieNW",
	"license": "MIT",
	"dependencies": {
		"cli-color": "^2.0.3",
		"prettier": "^3.6.2"
	},
	"devDependencies": {
		"@angablue/exe": "^2.1.3"
	}
}
